{
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": true
    }
  },
  "legacyExplorer": false,
  "enableSSL": true,
  "documentRoot": "/",
  "upload": {
    "maxDiskUsage": 0.95,
    "directory": [ "server", "upload"],
    "cleanupTmpDir": true,
    "maxFileAge": 18000,
    "segmentDigits": 8
  }

}
