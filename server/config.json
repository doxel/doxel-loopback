{
  "restApiRoot": "/api",
  "host": "127.0.0.1",
  "port": 3000,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "enableSSL": false,
  "cookieSecret": "override me in config.local.json",
  "migrateAuthCookie": true,
  "documentRoot": "/app/",
  "upload": {
    "maxDiskUsage": 0.95,
    "directory": [ "..", "upload_dev"],
    "cleanupTmpDir": true,
    "maxFileAge": 18000,
    "segmentDigits": 8
  },
  "uploaderPath": "/var/www/doxel-uploader",
  "viewerPath": "/var/www/webglearth2"
}
