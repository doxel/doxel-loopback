{
  "html5Mode": true,
  "restApiRoot": "/api",
  "host": "127.0.0.1",
  "port": 3000,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "enableSSL": false,
  "cookieSecret": "override me in config.local.json",
  "sessionSecret": "override me in config.local.json",
  "migrateAuthCookie": true,
  "documentRoot": "app/",
  "upload": {
    "maxDiskUsage": 0.95,
    "directory": [
      "server",
      "upload_dev"
    ],
    "imagesDir": "original_images",
    "imageExtension": "jpeg",
    "cleanupTmpDir": true,
    "maxFileAge": 18000,
    "segmentDigits": 8
  },
  "uploaderPath": "server/doxel-uploader",
  "viewerPath": "server/doxel-viewer",
  "viewer" : {
    "folders": ["viewer", "potree", "PMVS"]
  },
  "tileProxyConfig": {
    "middleware": true,
    "urlFromQueryString": false,
    "routes": {
      "osm": {
        "url": [
          "a.tile.openstreetmap.org",
          "b.tile.openstreetmap.org",
          "c.tile.openstreetmap.org"
        ]
      },
      "blue-marble": {
        "url": ["www.doxel.org/doxel-viewer/upload/blue-marble"]
      },
      "stamen": {
        "url": [
           "a.tile.stamen.com",
           "b.tile.stamen.com",
           "c.tile.stamen.com"
        ]
      }
    }
  }
}
